--- /dev/null
+++ b/etc/openstack_deploy/openstack_user_config.yml
@@ -0,0 +1,90 @@
+---
+cidr_networks:
+  dbaas: 172.29.232.0/22
+  lbaas: 172.29.252.0/22
+  container: 172.29.236.0/22
+  tunnel: 172.29.240.0/22
+  storage: 172.29.244.0/22
+
+used_ips:
+  - "172.29.232.1,172.29.232.50"
+  - "172.29.232.100"
+  - "172.29.252.1,172.29.252.50"
+  - "172.29.252.100"
+  - "172.29.236.1,172.29.236.50"
+  - "172.29.236.100"
+  - "172.29.240.1,172.29.240.50"
+  - "172.29.240.100"
+  - "172.29.244.1,172.29.244.50"
+  - "172.29.244.100"
+  - "172.29.248.1,172.29.248.50"
+  - "172.29.248.100"
+
+global_overrides:
+  internal_lb_vip_address: osa2_ip
+  # The external IP is quoted simply to ensure that the .aio file can be used as input
+  # dynamic inventory testing.
+  external_lb_vip_address: 127.0.0.1
+  management_bridge: "br-mgmt"
+  provider_networks:
+    - network:
+        container_bridge: "br-phynet1"
+        container_type: "veth"
+        type: "flat"
+        net_name: "physnet1"
+        # For dpdk, we add physical interface manually after deployment
+        #network_interface: "phynet1_if"
+        group_binds:
+          - neutron_openvswitch_agent
+    - network:
+        container_bridge: "br-vxlan"
+        container_type: "veth"
+        container_interface: "eth10"
+        ip_from_q: "tunnel"
+        type: "vxlan"
+        range: "1:1000"
+        net_name: "vxlan"
+        group_binds:
+          - neutron_openvswitch_agent
+
+# galera, memcache, rabbitmq, utility
+shared-infra_hosts:
+  aio1:
+    ip: osa1_ip
+
+repo-infra_hosts:
+  aio1:
+    ip: osa1_ip
+
+log_hosts:
+  aio1:
+    ip: osa1_ip
+
+image_hosts:
+  aio1:
+    ip: osa1_ip
+
+compute-infra_hosts:
+  aio1:
+    ip: osa1_ip
+
+compute_hosts:
+  aio1:
+    ip: osa1_ip
+
+placement-infra_hosts:
+  aio1:
+    ip: osa1_ip
+
+network_hosts:
+  aio1:
+    ip: osa1_ip
+
+identity_hosts:
+  aio1:
+    ip: osa1_ip
+
+
+haproxy_hosts:
+  aio2:
+    ip: osa2_ip
