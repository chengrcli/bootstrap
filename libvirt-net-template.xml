<network ipv6='yes'>
  <name>private-{netid}</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='virbr_{netid}' stp='on' delay='0'/>
  <ip address='192.168.{netid}.1' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.168.{netid}.1' end='192.168.{netid}.254'/>
    </dhcp>
  </ip>
</network>

